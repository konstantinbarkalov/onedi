{
  "render": {
    "master": {
      "metricSize": 5,
      "pixelCount": 960
    },  
    "composes": {
      "ledlineA": {
        "pixelCount": 150,
        "mapping": [0, 1],
        "gain": 1,
        "gamma": 1,
        "limit": {
          "electric": 0.5,
          "heat": 0.5
        }
      }
    }
  },
  "io": {
    "input": {
      "analogA": {
        "type": "analog",
        "label": "global rotation",
        "initialValue": 0.5
      },
    
      "analogC": {
        "type": "analog",
        "label": "global punchness",
        "initialValue": 0.5
      },
      
      "analogD": {
        "type": "analog",
        "label": "hero punchness",
        "initialValue": 0.5
      },

      "analogE": {
        "type": "analog",
        "label": "punchness curve",
        "initialValue": 0.5
      },

      "analogB": {
        "type": "analog",
        "label": "colorness",
        "initialValue": 0.5
      },

      "switchA": {
        "type": "switch",
        "label": "electric protection",
        "initialValue": false
      },
      "switchB": {
        "type": "switch",
        "label": "heat protection",
        "initialValue": false
       },
       "switchC": {
        "type": "switch",
        "label": "colorness overcolor",
        "initialValue": true,
        "description": "Instagram-alike color overgain with cost of total dynamic range loss."
      },
      "switchD": {
        "type": "switch",
        "label": "colorness harder",
        "initialValue": false,
        "description": "Extra contrast for right end of colorness only."
      }
    },
    "output": {
      "statusledA": {
        "type": "statusled",
        "label": "system",
        "initialValue": false
      },
      "alertledB": {
        "type": "alertled",
        "label": "electric protection",
        "initialValue": true
      },
      "alertledC": {
        "type": "alertled",
        "label": "heat protection",
        "initialValue": true
      }
    }
  }
